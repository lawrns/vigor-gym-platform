{
  "name": "@vigor/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 7777",
    "start": "next start -p 3000",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:load:setup": "node tests/load/setup-load-test.js setup",
    "test:load:run": "npx artillery run tests/load/auth-load-test.yml",
    "test:load:report": "npx artillery run tests/load/auth-load-test.yml --output load-report.json && npx artillery report load-report.json",
    "test:load:analyze": "node tests/load/setup-load-test.js analyze load-report.json",
    "test:load:cleanup": "node tests/load/setup-load-test.js cleanup",
    "perf:lighthouse": "lhci autorun",
    "perf:k6:smoke": "k6 run tests/k6/smoke.js",
    "perf:k6:load": "k6 run --vus 50 --duration 5m tests/k6/load.js",
    "perf:otel:smoke": "node scripts/otel-smoke.mjs",
    "perf:all": "npm run perf:lighthouse && npm run perf:k6:smoke && npm run perf:otel:smoke",
    "selftest:auth": "node scripts/selftest-auth.mjs"
  },
  "dependencies": {
    "@opentelemetry/exporter-metrics-otlp-http": "^0.41.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.2",
    "@opentelemetry/instrumentation-express": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@vigor/shared": "*",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.5",
    "next-themes": "^0.3.0",
    "posthog-js": "^1.189.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "artillery": "^2.0.24",
    "autoprefixer": "^10.4.19",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.4"
  }
}
