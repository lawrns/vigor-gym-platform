    },
    {
      "name": "Error Scenarios",
      "item": [
        {
          "name": "Duplicate Check-in",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{deviceToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"memberId\": \"{{memberId}}\",\n  \"gymId\": \"{{gymId}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/checkins/scan",
              "host": ["{{baseUrl}}"],
              "path": ["checkins", "scan"]
            },
            "description": "Attempt to check-in member who is already checked in"
          },
          "response": [
            {
              "name": "Duplicate Check-in Error",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"memberId\": \"123e4567-e89b-12d3-a456-426614174000\",\n  \"gymId\": \"456e7890-f12b-34c5-d678-901234567890\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/checkins/scan",
                  "host": ["{{baseUrl}}"],
                  "path": ["checkins", "scan"]
                }
              },
              "status": "Conflict",
              "code": 409,
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"message\": \"Member is already checked in\",\n  \"code\": \"DUPLICATE_CHECKIN\",\n  \"existingVisit\": {\n    \"id\": \"789abcde-f012-3456-7890-123456789abc\",\n    \"checkIn\": \"2025-01-16T20:25:00Z\"\n  }\n}"
            }
          ]
        },
        {
          "name": "Expired Membership",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{deviceToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"memberId\": \"expired-member-id\",\n  \"gymId\": \"{{gymId}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/checkins/scan",
              "host": ["{{baseUrl}}"],
              "path": ["checkins", "scan"]
            },
            "description": "Attempt to check-in member with expired membership"
          },
          "response": [
            {
              "name": "Expired Membership Error",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"memberId\": \"expired-member-id\",\n  \"gymId\": \"456e7890-f12b-34c5-d678-901234567890\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/checkins/scan",
                  "host": ["{{baseUrl}}"],
                  "path": ["checkins", "scan"]
                }
              },
              "status": "Forbidden",
              "code": 403,
              "header": [
