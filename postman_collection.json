{
  "info": {
    "name": "Vigor Device Check-in API",
    "description": "Production API for gym device registration, authentication, and member check-in operations.",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{deviceToken}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:4002/v1",
      "type": "string"
    },
    {
      "key": "adminToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "deviceToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "deviceId",
      "value": "",
      "type": "string"
    },
    {
      "key": "deviceSecret",
      "value": "",
      "type": "string"
    },
    {
      "key": "visitId",
      "value": "",
      "type": "string"
    },
    {
      "key": "memberId",
      "value": "123e4567-e89b-12d3-a456-426614174000",
      "type": "string"
    },
    {
      "key": "gymId",
      "value": "456e7890-f12b-34c5-d678-901234567890",
      "type": "string"
    }
