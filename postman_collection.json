            "body": {
              "mode": "raw",
              "raw": "{\n  \"qrCode\": \"{\\\"type\\\":\\\"member\\\",\\\"id\\\":\\\"{{memberId}}\\\"}\",\n  \"gymId\": \"{{gymId}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/checkins/scan",
              "host": ["{{baseUrl}}"],
              "path": ["checkins", "scan"]
            },
            "description": "Scan member QR code for check-in"
          },
          "response": [
            {
              "name": "Active Member Success",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"qrCode\": \"{\\\"type\\\":\\\"member\\\",\\\"id\\\":\\\"123e4567-e89b-12d3-a456-426614174000\\\"}\",\n  \"gymId\": \"456e7890-f12b-34c5-d678-901234567890\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/checkins/scan",
                  "host": ["{{baseUrl}}"],
                  "path": ["checkins", "scan"]
                }
              },
              "status": "Created",
              "code": 201,
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"visit\": {\n    \"id\": \"789abcde-f012-3456-7890-123456789abc\",\n    \"checkIn\": \"2025-01-16T20:30:00Z\",\n    \"member\": {\n      \"id\": \"123e4567-e89b-12d3-a456-426614174000\",\n      \"firstName\": \"Juan\",\n      \"lastName\": \"PÃ©rez\",\n      \"email\": \"juan.perez@example.com\"\n    },\n    \"gym\": {\n      \"id\": \"456e7890-f12b-34c5-d678-901234567890\",\n      \"name\": \"Vigor Gym Centro\"\n    }\n  },\n  \"state\": \"OK\",\n  \"message\": \"Check-in successful\"\n}"
            }
          ],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('visitId', response.visit.id);",
                  "    pm.test('Check-in successful', function () {",
                  "        pm.expect(response.visit.id).to.be.a('string');",
                  "        pm.expect(response.state).to.equal('OK');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Scan Member (Manual ID)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{deviceToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"memberId\": \"{{memberId}}\",\n  \"gymId\": \"{{gymId}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/checkins/scan",
              "host": ["{{baseUrl}}"],
              "path": ["checkins", "scan"]
            },
            "description": "Manual member ID entry for check-in"
          }
        },
