  "item": [
    {
      "name": "Device Management",
      "item": [
        {
          "name": "Register Device",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Kiosk Entrada Principal\",\n  \"locationId\": \"123e4567-e89b-12d3-a456-426614174000\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/devices/register",
              "host": ["{{baseUrl}}"],
              "path": ["devices", "register"]
            },
            "description": "Register a new kiosk device for a gym location. Requires admin authentication."
          },
          "response": [
            {
              "name": "Success",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"name\": \"Kiosk Entrada Principal\",\n  \"locationId\": \"123e4567-e89b-12d3-a456-426614174000\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/devices/register",
                  "host": ["{{baseUrl}}"],
                  "path": ["devices", "register"]
                }
              },
              "status": "Created",
              "code": 201,
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"device\": {\n    \"id\": \"987fcdeb-51a2-43d1-9f4e-123456789abc\",\n    \"name\": \"Kiosk Entrada Principal\",\n    \"locationId\": \"123e4567-e89b-12d3-a456-426614174000\",\n    \"createdAt\": \"2025-01-16T20:30:00Z\"\n  },\n  \"deviceSecret\": \"a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456\"\n}"
            }
          ],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('deviceId', response.device.id);",
                  "    pm.collectionVariables.set('deviceSecret', response.deviceSecret);",
                  "    pm.test('Device registered successfully', function () {",
                  "        pm.expect(response.device.id).to.be.a('string');",
                  "        pm.expect(response.deviceSecret).to.be.a('string');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Authenticate Device",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"deviceId\": \"{{deviceId}}\",\n  \"deviceSecret\": \"{{deviceSecret}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/devices/auth",
              "host": ["{{baseUrl}}"],
              "path": ["devices", "auth"]
            },
            "description": "Exchange device credentials for JWT token"
          },
          "response": [
            {
              "name": "Success",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"deviceId\": \"987fcdeb-51a2-43d1-9f4e-123456789abc\",\n  \"deviceSecret\": \"a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/devices/auth",
                  "host": ["{{baseUrl}}"],
                  "path": ["devices", "auth"]
                }
              },
              "status": "OK",
              "code": 200,
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"deviceToken\": \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...\",\n  \"device\": {\n    \"id\": \"987fcdeb-51a2-43d1-9f4e-123456789abc\",\n    \"name\": \"Kiosk Entrada Principal\",\n    \"companyId\": \"456e7890-f12b-34c5-d678-901234567890\"\n  },\n  \"expiresIn\": 86400\n}"
            }
          ],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('deviceToken', response.deviceToken);",
                  "    pm.test('Device authenticated successfully', function () {",
                  "        pm.expect(response.deviceToken).to.be.a('string');",
                  "        pm.expect(response.expiresIn).to.be.a('number');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "Check-ins",
      "item": [
        {
          "name": "Scan Member (QR Code)",
